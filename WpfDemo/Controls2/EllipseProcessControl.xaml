<UserControl x:Class="WpfDemo.Controls2.EllipseProcessControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:WpfDemo.Controls2"
             mc:Ignorable="d"
             d:DesignHeight="450"
             d:DesignWidth="800">
    <Grid>
        <!--圆环-->
        <Path Stroke="#1e305a"
              StrokeDashArray="0.5,0.15"
              StrokeThickness="10">
            <Path.Data>
                <EllipseGeometry Center="100,100"
                                 RadiusX="50"
                                 RadiusY="50"></EllipseGeometry>
            </Path.Data>
        </Path>

        <!--<Path Stroke="Green"
              StrokeDashArray="0.5,0.15"
              StrokeThickness="10">
            <Path.Data>
                <EllipseGeometry Center="100,100"
                                 RadiusX="50"
                                 RadiusY="50"></EllipseGeometry>
            </Path.Data>
        </Path>-->

    </Grid>
</UserControl>
